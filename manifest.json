{
  "name": "Tab Saver",
  "description" : "",
  "version": "1.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": ["tabs", "storage", "unlimitedStorage"],
  "options_page": "list.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
